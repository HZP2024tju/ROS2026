<robot name = "MYcar">
    <link name = "base_footprint">
        <visual>
            <geometry>
                <sphere radius="0.001" />
            </geometry>
        </visual>
    </link>  
    <link name="base_link">
        <visual>
            <geometry>
                <cylinder radius="0.1" length="0.08" />
            </geometry>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <material name="BL_clolor">
                <color rgba="1.0 0.5 0.2 0.5" />
            </material>
        </visual>
    </link>
    <joint name="L1" type="fixed">
        <parent link="base_footprint" />
        <child link="base_link" />
        <origin xyz="0 0 0.055" />
    </joint>
    <link name="left_wheel">
        <visual>
            <origin xyz="0 0 0" rpy="1.5708 0 0" />
            <geometry>
                <cylinder radius="0.055" length="0.01" />
            </geometry>
            <material name="wheel_color">
                <color rgba="1 1 1 1" />
            </material>
        </visual>
    </link>
    <joint name="wheel1" type="fixed">
        <parent link="base_link" />
        <child link="left_wheel" />
        <origin xyz="0 0.105 0" rpy="0 0 0" />
        <axis xyz="0 1 0" />
    </joint>

    <link name="f_wheel">
        <visual>
            <origin xyz="0 0 0" rpy="0 1.5708 0" />
            <geometry>
                <cylinder radius="0.055" length="0.01" />
            </geometry>
            <material name="wheel_color">
                <color rgba="1 1 1 1" />
            </material>
        </visual>
    </link>
    <joint name="wheel2" type="fixed">
        <parent link="base_link" />
        <child link="f_wheel" />
        <origin xyz="0.105 0 0" rpy="0 0 0" />
        <axis xyz="1 0 0" />
    </joint>

    <link name="right_wheel">
        <visual>
            <origin xyz="0 0 0" rpy="0 1.5708 0" />
            <geometry>
                <cylinder radius="0.055" length="0.01" />
            </geometry>
            <material name="wheel_color">
                <color rgba="1 1 1 1" />
            </material>
        </visual>
    </link>
    <joint name="wheel3" type="fixed">
        <parent link="base_link" />
        <child link="right_wheel" />
        <origin xyz="-0.105 0 0" rpy="0 0 0" />
        <axis xyz="1 0 0" />
    </joint>

    <link name="b_wheel">
        <visual>
            <origin xyz="0 0 0" rpy="1.5708 0 0" />
            <geometry>
                <cylinder radius="0.055" length="0.01" />
            </geometry>
            <material name="wheel_color">
                <color rgba="1 1 1 1" />
            </material>
        </visual>
    </link>
    <joint name="wheel4" type="fixed">
        <parent link="base_link" />
        <child link="b_wheel" />
        <origin xyz="0 -0.105 0" rpy="0 0 0" />
        <axis xyz="1 0 0" />
    </joint>
</robot>